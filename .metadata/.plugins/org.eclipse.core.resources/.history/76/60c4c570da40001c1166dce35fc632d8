
public class Main {

	public static void main(String[] args) {
		
		System.out.println("\"I love pizza\"");
		System.out.println("It's real good!");
		
//		This is a comment
/*
 *		This
 *		is 
 *		a 
 *		multi-line
 *		comment
 */
//		print() will print on one line
//		println() will print one one line and create a new line for the next printed text
/*
 * 		\ is the start of a escape sequence
 * 		\n is an escape sequence for a new line
 * 		\t is an escape sequence for a tab
 *		\" is an escape sequence for a quotation
 */
	}

}
